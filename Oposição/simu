    %pushi/vec4 10, 0, 8;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5f87bee7eb20_0, 4, 8;
    %pushi/vec4 236, 0, 8;
    %ix/load 4, 8, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5f87bee7eb20_0, 4, 8;
    %pushi/vec4 30, 0, 8;
    %ix/load 4, 16, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5f87bee7eb20_0, 4, 8;
    %pushi/vec4 216, 0, 8;
    %ix/load 4, 24, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5f87bee7eb20_0, 4, 8;
