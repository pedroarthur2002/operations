TD_oposicao_matriz_tb.print_matrix ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0000015435cdd0f0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0000015435cdd0f0_0;
    %load/vec4 v0000015435c7ce50_0;
    %load/vec4 v0000015435c7ce50_0;
    %mul;
    %cmp/s;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0000015435cdd410_0;
    %load/vec4 v0000015435cdd0f0_0;
    %muli 8, 0, 32;
    %part/s 8;
    %vpi_call 2 26 "$write", "%4d ", S<0,vec4,s8> {1 0 0};
    %load/vec4 v0000015435cdd0f0_0;
    %addi 1, 0, 32;
    %load/vec4 v0000015435c7ce50_0;
    %mod/s;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %vpi_call 2 28 "$write", "\012" {0 0 0};
T_0.2 ;
    %load/vec4 v0000015435cdd0f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0000015435cdd0f0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 30 "$write", "\012" {0 0 0};
    %end;
